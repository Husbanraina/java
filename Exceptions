import java.util.*;
public class TryCatchExample {  
  
    public static void main(String[] args) {  
        try  
        {  
        int data=20/0; 
  
        }  
              
        catch(ArrayIndexOutOfBoundsException e)  
        {  
            System.out.println(e);  
        }  
        System.out.println("rest of the code");  
    }  
      
}  
