import java.util.*;
public class TryCatchExamples {  
  
    public static void main(String[] args) {  
        try  
        {  
        int data=20/0; 
        }  
               
        catch(Exception e)  
        {  
                   
            System.out.println("Can't be divided by zero");  
        }  
    }  
      
}  
